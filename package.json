{
  "name": "template-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "pnpm run --filter ./examples dev",
    "build:components": "gulp -f packages/components/script/build.ts",
    "publish:components": "gulp -f packages/components/script/publish.ts",
    "build:utils": "pnpm run --filter ./packages/utils build",
    "publish:utils": "gulp -f packages/utils/script/publish.ts",
    "docs:dev": "pnpm run --filter ./docs docs:dev",
    "docs:build": "pnpm run --filter ./docs docs:build",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:eslint": "eslint  --fix --ext .vue,.js,.jsx,.ts,.tsx ./",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint  \"**/*.{css,scss,vue}\" --fix",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "release-it"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/gulp": "^4.0.17",
    "@types/gulp-autoprefixer": "^0.0.37",
    "@types/gulp-less": "^0.0.36",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.4.2",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/runtime-core": "^3.4.38",
    "@vue/test-utils": "^2.4.6",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.4",
    "element-plus": "^2.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-less": "^5.0.0",
    "gulp-sass": "^5.1.0",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.5",
    "javascript-obfuscator": "^4.1.1",
    "lint-staged": "^15.2.9",
    "minimist": "^1.2.8",
    "postcss": "^8.4.41",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "rollup-plugin-obfuscator": "^1.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "stylelint": "^16.8.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "sucrase": "^3.35.0",
    "terser": "^5.31.6",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-dts": "4.0.3",
    "vitest": "^2.0.5",
    "vue": "^3.4.38",
    "vue-tsc": "^2.0.29"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{cjs,json}": [
      "prettier --write"
    ],
    "*.{vue,html}": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
